{
    "manifest_version": 2,
    "name": "FingCorrect",
    "version": "1.0",
    "description": "Generates and applies fingerprint based on config",
    "permissions": [
      "webRequest",
      "webRequestBlocking",
      "activeTab",
      "storage",
      "<all_urls>"
    ],
    "background": {
        "scripts": ["background.js"],
        "persistent": true
    },
    "browser_action": {
        "default_title": "FingCorrect",
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"],
            "run_at": "document_start"
        }
    ],
    "icons": {
      "48": "icon.png",
      "96": "icon@2x.png"
    },
    "web_accessible_resources": ["data/generation_config.json"]
  }
  